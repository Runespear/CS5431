<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.cs5431.view.ConnectController">
    <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
        <Text strokeType="OUTSIDE" strokeWidth="0.0"
              text="Pretty Secure File Sharing (◡‿◡✿)">
            <font>
                <Font size="18.0"/>
            </font>
        </Text>
        <ChoiceBox fx:id="serverDropdown" prefWidth="150.0"/>
        <Button fx:id="connectButton" mnemonicParsing="false"
                text="Connect"/>
    </VBox>
</AnchorPane>
