<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.cs5431_client.view.PrivViewController">
    <ToolBar prefHeight="46.0" prefWidth="600.0"/>
    <HBox layoutY="46.0" prefHeight="34.0" prefWidth="600.0">
        <Label prefWidth="350.0" text="Username"/>
        <Label prefWidth="100.0" text="Can Edit"/>
        <Label prefWidth="100.0" text="Can View"/>
        <Label prefWidth="50.0" text="Delete"/>
    </HBox>
    <ListView fx:id="listViewPriv" layoutY="70.0" prefHeight="330.0"
              prefWidth="600.0"/>
    <StackPane prefHeight="46.0" prefWidth="600.0">
        <Text fx:id="txtFilename" text="Sharing for filename">
            <font>
                <Font size="14.0"/>
            </font>
        </Text>
        <StackPane alignment="CENTER_RIGHT" prefHeight="46.0"
                   prefWidth="600.0" StackPane.alignment="CENTER_RIGHT">
            <HBox alignment="CENTER_RIGHT"
                  StackPane.alignment="CENTER_RIGHT">
                <ImageView fx:id="imgAdd" fitHeight="40.0"
                           fitWidth="40.0" pickOnBounds="true"
                           preserveRatio="true">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                    <Image url="@../assets/ic_person_add.png"/>
                </ImageView>
                <ImageView fx:id="imgExit" fitHeight="40.0"
                           fitWidth="40.0" pickOnBounds="true"
                           preserveRatio="true">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                    <Image url="@../assets/ic_clear.png"/>
                </ImageView>
            </HBox>
        </StackPane>
    </StackPane>
</AnchorPane>
