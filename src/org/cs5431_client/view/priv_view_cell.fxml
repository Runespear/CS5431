<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<HBox fx:id="hBox" prefWidth="600" xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/8">

    <Label fx:id="username" prefHeight="17.0" prefWidth="350.0"/>
    <CheckBox fx:id="editCheck" mnemonicParsing="false" prefWidth="100.0"/>
    <CheckBox fx:id="viewCheck" mnemonicParsing="false" prefWidth="100.0"/>
    <ImageView fx:id="imgDelete" pickOnBounds="true" preserveRatio="true">
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
        <Image url="@../assets/ic_delete.png"/>
    </ImageView>
</HBox>
